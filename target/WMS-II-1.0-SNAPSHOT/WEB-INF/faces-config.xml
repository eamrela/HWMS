<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        
        
        
         <navigation-rule>
            <from-view-id>/app/orders/create_order_in_bound.xhtml</from-view-id>
            <navigation-case>
            <from-action>#{ordersController.prepareCreateInBound}</from-action>
            <from-outcome>com.huawei.wms.ii.entities.Orders[ orderId=null ]</from-outcome>
            <to-view-id>/app/orders/create_order_in_bound.xhtml</to-view-id>
          </navigation-case>
        </navigation-rule>
        
         <navigation-rule>
            <from-view-id>/app/orders/create_order_transfer.xhtml</from-view-id>
            <navigation-case>
            <from-action>#{ordersController.prepareCreateTransfer}</from-action>
            <from-outcome>com.huawei.wms.ii.entities.Orders[ orderId=null ]</from-outcome>
            <to-view-id>/app/orders/create_order_transfer.xhtml</to-view-id>
          </navigation-case>
        </navigation-rule>
        
        <navigation-rule>
            <from-view-id>/app/orders/create_order_issue.xhtml</from-view-id>
            <navigation-case>
            <from-action>#{ordersController.prepareCreateIssue}</from-action>
            <from-outcome>com.huawei.wms.ii.entities.Orders[ orderId=null ]</from-outcome>
            <to-view-id>/app/orders/create_order_issue.xhtml</to-view-id>
          </navigation-case>
        </navigation-rule>
        
    </application>
</faces-config>
